<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress files="^(?!.*tech[/\\]unideb[/\\]backend[/\\]controller).*$" id="controllerName" />
    <suppress files="^(?!.*tech[/\\]unideb[/\\]backend[/\\]config).*$" id="configName" />
    <suppress files="^(?!.*tech[/\\]unideb[/\\]backend[/\\]repository).*$" id="repositoryName" />
    <suppress files="^(?!.*tech[/\\]unideb[/\\]backend[/\\]service).*$" id="serviceName" />
    <suppress files="^(?!.*tech[/\\]unideb[/\\]backend[/\\]service[/\\]impl).*$" id="serviceImplName" />

    <suppress files="^(?!.*tech[/\\]unideb[/\\]backend[/\\](service|repository)).*$" id="javadocMethods" />
    <suppress files="^(?!.*tech[/\\]unideb[/\\]backend[/\\](component|model)).*$" id="javadocTypes" />
</suppressions>